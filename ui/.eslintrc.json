{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google", "eslint-config-prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "require-jsdoc": 0,
    "@typescript-eslint/explicit-function-return-type": 2,
    "max-len": [2, { "code": 120 }]
  }
}
